[user]
  name = Michael Cooney
  email = mike.cooney@gmail.com
[color]
  branch = auto
  diff = auto
  status = auto
[core]
  excludesfile = /Users/michael/.gitignore
[alias]
  ai = "add -i"
  ap = "add -p"
  l = "log --pretty=format:'%Cgreen%h%Creset %Cblue<%an>%Creset %s %Cred(%ar)%Creset %Cgreen%d'"
  s = status
  st = !tig status
  p = pull
  pm = "push mine"
  po = "push origin"
  f = fetch
  fo = "fetch origin"
  fa = "fetch --all"
  d = diff
  ds = "diff --staged"
  c = "commit -v"
  ca = "commit -av"
  b = branch -v
  br = "branch -r"
  bro = "!git branch -r | grep origin"
  co = checkout
  cob = "checkout -b"
  watchanged = whatchanged
[merge]
  tool = vimdiff
[push]
  default = upstream
[branch]
  autosetuprebase = always
